import java.util.Scanner;
class Reversenum{
 public static void main(String[] args){
  Scanner sc = new Scanner(System.in);
  System.out.println("Please enter the Number ");
  int n = sc.nextInt();
  int s = 0,r = 0;


  while(n>0){
	r = n%10;
	s = s*10 + r;
	n /= 10;

}
  System.out.println("the Reversed of the Number is : " + s);
}
}
